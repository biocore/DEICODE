# this file is based on the examples provided on scikit-learn's .coveragerc

[run]
omit =
    */tests*
    */__init__.py
    */deicode/optspace.py
    */deicode/utils.py 
source = DEICODE
branch = True
include = */deicode/*

[report]
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
omit =
    */tests*
    */__init__.py
    */deicode/optspace.py
    */deicode/utils.py     
